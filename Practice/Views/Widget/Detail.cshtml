
@model Practice.Models.Widget

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@Model.Name</h2>
<ul>
    <li>Widget Type: @Model.WidgetType.TypeOfWidget </li>
    <li>Date Added To Stock: @Model.DateAddedToStock.ToShortDateString()</li>
    <li>Number Currently In Stock: @Model.NumberInStock</li>
</ul>


